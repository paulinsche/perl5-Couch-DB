#
# source dieses File.
#
export PATH="${HOME}/.plenv/shims:${PATH}"
export PLENV_SHELL=bash
source "${HOME}/.plenv/libexec/../completions/plenv.bash"

plenv() {
  local command

  command="$1"
  if [ "$#" -gt 0 ]; then
    shift
  fi

  case "$command" in
  rehash|shell)
    eval "`plenv "sh-$command" "$@"`";;
  *)
    command plenv "$command" "$@";;
  esac
}

exec plenv use 5.40.2@perl5-Couch-DB
